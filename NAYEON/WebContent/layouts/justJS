<script type="text/javascript"> 
new Swiper('.swiper-container', {

   slidesPerView : 3, 
   spaceBetween : 30, 


   loop : true, 

   pagination : { 
      el : '.swiper-pagination',
      clickable : true, 
   },
   navigation : { // 네비게이션
      nextEl : '.swiper-button-next', 
      prevEl : '.swiper-button-prev', 
   },
});
</script>